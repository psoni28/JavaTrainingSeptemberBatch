package abstractPack;

public class Circle extends Shape {

	double r;

	Circle() {
		r = 12.32;
	}

	@Override
	public void findArea() {
		super.area = 3.14 * this.r * this.r;

		super.displayedArea();
	}

	public static void main(String[] args) {

		Shape reference = new Circle();

		reference.findArea();

	}

}
