package stringPack;

public class StringConstantPoolDemo {

	public static void main(String[] args) {

		String name1 = "India";

		String name2 = "India";

		String name3 = new String("India");

		String name4 = "is a great country";

		System.out.println(name1 == name3);

		name1 = name1.concat(name4);

		System.out.println(name1);

		String name5 = "is a great country";

		System.out.println(name4 == name5);

		// String constant pool applicable for all literals mentions in the double
		// quaotes only or create via double quotes

	}

}
