package training;

public class Employee {

	// data member
	int employeeId = 101;

	double salary = 20000;

	void getSalaryInfo() {

		System.out.println(salary);
	}

	void getEmployeedId() {

		System.out.println(employeeId);
	}

	public static void main(String[] args) {
		Employee obj1 = new Employee();
		
		
		obj1.getEmployeedId();
		
		obj1.getSalaryInfo();

	}

}
