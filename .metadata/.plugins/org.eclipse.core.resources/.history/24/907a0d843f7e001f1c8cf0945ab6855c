package twoDimestinalArrayPack;

public class TwoDimestionalArray {

	public static void main(String[] args) {

		int[][] arr2 = new int[3][3];

		arr2[0][0] = 10;
		arr2[0][1] = 20;
		arr2[0][2] = 30;

		arr2[1][0] = 40;
		arr2[1][1] = 50;
		arr2[1][2] = 60;

		arr2[2][0] = 70;
		arr2[2][1] = 80;
		arr2[2][2] = 90;

		for (int row = 0; row < arr2.length; row++) {v// row =2

			for (int col = 0; col < arr2.length; col++) {

				System.out.print(arr2[row][col]+"  ");
			}
		
		System.out.println();

		}

	}

}
