package operatorsPack;

public class RelationalOperator {

	public static void main(String[] args) {
		int a = 10, b = 5;

		boolean result;

		result = (a == b);

		System.out.println(result);

		result = (a != b);
		System.out.println(result);

		result = (a > b);
		System.out.println(result);

		result = (a < b);
		System.out.println(result);

	}

}
